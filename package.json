{
  "name": "youtube-video-tag-viewer",
  "description": "Chrome Extension for Viewing Video Tags on YouTube.",
  "version": "0.0.6",
  "dependencies": {
    "@fiahfy/semaphore": "^0.0.2"
  },
  "devDependencies": {
    "@fiahfy/eslint-config": "^0.0.18",
    "@fiahfy/prettier-config": "^0.0.2",
    "@fiahfy/tsconfig": "^0.0.1",
    "@types/chrome": "^0.0.206",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.31.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "mv3-hot-reload": "^0.2.7",
    "prettier": "^2.8.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1"
  },
  "private": true,
  "productName": "Video Tag Viewer for YouTube",
  "scripts": {
    "build": "webpack",
    "dev": "concurrently npm:watch:*",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lint:prettier": "prettier -c --ignore-path .gitignore .",
    "package": "npm run build && mkdir -p dist && zip -r dist/archive.zip app",
    "prepare": "husky install",
    "test": "npm run lint",
    "watch:dist": "mv3-hot-reload",
    "watch:src": "webpack -w --config webpack.config.dev.js"
  }
}
