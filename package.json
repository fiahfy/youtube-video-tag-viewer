  "version": "0.0.7",
  "dependencies": {
    "@fiahfy/semaphore": "0.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@crxjs/vite-plugin": "2.2.0",
    "@fiahfy/biome-config": "0.0.2",
    "@tsconfig/vite-react": "7.0.1",
    "@types/chrome": "0.1.11",
    "husky": "9.1.7",
    "typescript": "5.9.3",
    "vite": "7.1.11",
    "vite-plugin-zip-pack": "1.2.4",
    "vite-tsconfig-paths": "5.1.4"
  },
  "packageManager": "yarn@4.10.3",
  "private": true,
  "productName": "Video Tag Viewer for YouTube",
