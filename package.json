{
  "name": "youtube-video-tag-viewer",
  "description": "Chrome Extension for Viewing Video Tags on YouTube.",
  "version": "0.0.6",
  "dependencies": {
    "@fiahfy/semaphore": "^0.0.2"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.16",
    "@fiahfy/eslint-config": "^0.0.20",
    "@fiahfy/prettier-config": "^0.0.3",
    "@fiahfy/tsconfig": "^0.0.2",
    "@types/chrome": "^0.0.235",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "private": true,
  "productName": "Video Tag Viewer for YouTube",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier -c --ignore-path .gitignore .",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "package": "zip -r extension.zip dist",
    "prepare": "husky install",
    "test": "npm run format && npm run lint"
  },
  "type": "module"
}
